	<!-- Material Reactions -->
	<xsd:complexType name="MaterialReactions">
		<!-- Reaction Docs --><xsd:annotation><xsd:documentation><![CDATA[<br>```cpp<br>class Reaction {<br>&emsp;&emsp;&emsp;&emsp;bool fast_reaction = 0; //  Makes the reaction go faster<br>&emsp;&emsp;&emsp;&emsp;float probability = 0; //  If the cells are selected this is the percentage chance the reaction occurs. This can have up to 2 decimal places, further precision does nothing.<br>&emsp;&emsp;&emsp;&emsp;std::string input_cell1; //  The first cell in the reaction<br>&emsp;&emsp;&emsp;&emsp;std::string output_cell1; //  The material input_cell1 is converted to<br>&emsp;&emsp;&emsp;&emsp;std::string input_cell2; //  The second cell in the reaction<br>&emsp;&emsp;&emsp;&emsp;std::string output_cell2; //  The material input_cell2 is converted to<br>&emsp;&emsp;&emsp;&emsp;std::string input_cell3 = ""; //  If this is empty its a 2 cell reaction, if it's not then it's a 3 cell reaction, and output_cell3 must be set<br>&emsp;&emsp;&emsp;&emsp;std::string output_cell3 = ""; //  If this is empty its a 2 cell reaction, if it's not then it's a 3 cell reaction, and input_cell3 must be set<br>&emsp;&emsp;&emsp;&emsp;std::string cosmetic_particle = "";<br>&emsp;&emsp;&emsp;&emsp;int req_lifetime = 0; //  Only do the reaction if input_cell1 has lived longer than this<br>&emsp;&emsp;&emsp;&emsp;int blob_radius1 = 0; //  Radius of pixels of input_cell1 to react<br>&emsp;&emsp;&emsp;&emsp;int blob_radius2 = 0; //  Radius of pixels of input_cell1 to react<br>&emsp;&emsp;&emsp;&emsp;bool blob_restrict_to_input_material1 = 1;<br>&emsp;&emsp;&emsp;&emsp;bool blob_restrict_to_input_material2 = 1;<br>&emsp;&emsp;&emsp;&emsp;bool destroy_horizontally_lonely_pixels = 0;<br>&emsp;&emsp;&emsp;&emsp;REACTION_DIRECTION direction = none; //  Only do the reaction if input_cell2 is in this direction from input_cell2<br>&emsp;&emsp;&emsp;&emsp;bool convert_all = 0; //  If true floodfills the connected region of each input cell type and converts them to their respective output<br>&emsp;&emsp;&emsp;&emsp;std::string entity = ""; //  Loads this entity at the reaction coords when it happens<br>&emsp;&emsp;&emsp;&emsp;float audio_fx_volume_1 = 0;<br>};<br>```<br>]]></xsd:documentation></xsd:annotation><!-- Reaction Docs -->
		<xsd:choice>
			<xsd:element name="ExplosionConfig" type="ConfigExplosion" minOccurs="0" />
		</xsd:choice>
		<!-- Reaction Attributes -->
		<xsd:attribute name="fast_reaction" type="NoitaBool" default="0">
			<xsd:annotation>
