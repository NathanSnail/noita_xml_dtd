	<!-- Material Graphics Type -->
	<xsd:complexType name="MaterialGraphics" mixed="true">
		<xsd:choice>
			<xsd:element name="Edge" type="MaterialGraphicsEdge" minOccurs="0" />
		</xsd:choice>
		<xsd:attribute name="color" type="Hex8" default="00000000">
