		<xsd:attribute name="ground_penetration_coeff" type="xsd:decimal" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>float ground_penetration_coeff = 0; // [0, 5] if > 0, this, along with VelocityComponent.mass affects how far we penetrate in materials<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="ground_penetration_max_durability_to_destroy" type="xsd:int" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>int ground_penetration_max_durability_to_destroy = 0; //  if 0, will not penetrate into materials with durability greater than this<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="go_through_this_material" type="xsd:string" default="">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>std::string go_through_this_material; //  if set, we never collide with this material<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="do_moveto_update" type="NoitaBool" default="1">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>bool do_moveto_update = 1; //  this should probably be true, to get normal projectile behaviour, but you might want to disable this for some physics-based projectiles, like bombs<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="on_death_duplicate_remaining" type="xsd:int" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>int on_death_duplicate_remaining = 0; //  if greater than 0, the projectile creates two clones of itself on death. 'on_death_duplicate_remaining' on the clones is reduced by one<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="on_death_gfx_leave_sprite" type="NoitaBool" default="1">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>bool on_death_gfx_leave_sprite = 1; //  if true, finds all the sprites and leaves as sand cells into the grid<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="on_death_explode" type="NoitaBool" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>bool on_death_explode = 0; //  if true, does explosion with config_explosion<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="on_death_emit_particle" type="NoitaBool" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>bool on_death_emit_particle = 0; //  if true, emits on_death_emit_particle_type on death<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="on_death_emit_particle_count" type="xsd:int" default="1">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>int on_death_emit_particle_count = 1; //  how many particles should we emit<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="die_on_liquid_collision" type="NoitaBool" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>bool die_on_liquid_collision = 0; //  if true, dies on collision with liquids<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="die_on_low_velocity" type="NoitaBool" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>bool die_on_low_velocity = 0; //  if true, dies when speed goes below die_on_low_velocity_limit<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="die_on_low_velocity_limit" type="xsd:decimal" default="50">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>float die_on_low_velocity_limit = 50; //  please see die_on_low_velocity<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="on_death_emit_particle_type" type="xsd:string" default="">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>std::string on_death_emit_particle_type;<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="on_death_particle_check_concrete" type="NoitaBool" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>bool on_death_particle_check_concrete = 0; //  if you want it to stick as concrete, you should enable this<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="ground_collision_fx" type="NoitaBool" default="1">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>bool ground_collision_fx = 1; //  if 1, spurt some particles when colliding with mortals<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="explosion_dont_damage_shooter" type="NoitaBool" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>bool explosion_dont_damage_shooter = 0; //  if true, explosion doesn't damage the entity who shot this<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="on_death_item_pickable_radius" type="xsd:decimal" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>float on_death_item_pickable_radius = 0; //  if > 0, makes items closer than this radius pickable on death<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="penetrate_world" type="NoitaBool" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>bool penetrate_world = 0; //  if true, the projectile doesn't collide with ground, liquids, physical objects etc<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="penetrate_world_velocity_coeff" type="xsd:decimal" default="0.6">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>float penetrate_world_velocity_coeff = 0.6; //  if 'penetrate_world' is true, the projectile moves with a velocity multiplied by this value when inside world<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="penetrate_entities" type="NoitaBool" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>bool penetrate_entities = 0; //  if true, the projectile doesn't stop when it collides with entities. damages each entity only once<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="on_collision_die" type="NoitaBool" default="1">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>bool on_collision_die = 1; //  if true, this is killed as soon as it hits the ground<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="on_collision_remove_projectile" type="NoitaBool" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>bool on_collision_remove_projectile = 0; //  if true, ProjectileComponent is removed from the entitiy<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="on_collision_spawn_entity" type="NoitaBool" default="1">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>bool on_collision_spawn_entity = 1; //  if true, spawns the spawn_entity<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="spawn_entity" type="xsd:string" default="">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>std::string spawn_entity; //  this is spawned if hit something an on_collision_spawn_entity = 1<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="spawn_entity_is_projectile" type="NoitaBool" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>bool spawn_entity_is_projectile = 0; //  if true, will use ShootProjectile instead of LoadEntity()<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="physics_impulse_coeff" type="xsd:decimal" default="300">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>float physics_impulse_coeff = 300; //  projectile applies an impulse to physics bodies it hits. Impulse = physics_impulse_coeff * velocity<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="damage_every_x_frames" type="xsd:int" default="-1">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>int damage_every_x_frames = -1; //  if set != -1, will only do damage every x frames, used for fields and such, which would otherwise do damage every frame<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="damage_scaled_by_speed" type="NoitaBool" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>bool damage_scaled_by_speed = 0; //  if 1, damage is multiplied by (projectile speed / original projectile speed) ratio<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="damage_scale_max_speed" type="xsd:decimal" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>float damage_scale_max_speed = 0; //  if > 0 and damage_scaled_by_speed = 1, will use this instead of mInitialSpeed when calculating the damage<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="collide_with_entities" type="NoitaBool" default="1">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>bool collide_with_entities = 1; //  if 1, looks for entities with tag, collide_with_tag and collides with them, giving them damage<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="collide_with_tag" type="xsd:string" default="hittable">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>std::string collide_with_tag = "hittable"; //  default: mortal, if you needed can be changed to something more specific<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="dont_collide_with_tag" type="xsd:string" default="">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>std::string dont_collide_with_tag; //  if set will ignore entities with this tag<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="collide_with_shooter_frames" type="xsd:int" default="-1">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>int collide_with_shooter_frames = -1; //  remember friendly_fire 1, if -1 won't collide with shooter at all, otherwise uses the value as frame count and while it's running won't damage the shooter <br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="friendly_fire" type="NoitaBool" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>bool friendly_fire = 0; //  if true, will damage same herd id<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="damage" type="xsd:decimal" default="1">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>float damage = 1; //  how much Projectile damage does this do when it hits something<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="knockback_force" type="xsd:decimal" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>float knockback_force = 0; //  How far do entities get thrown if a knockback occurs. final_knockback = ProjectileComponent.knockback_force * VelocityComponent.mVelocity * VelocityComponent.mass / who_we_hit.mass<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="ragdoll_force_multiplier" type="xsd:decimal" default="0.025">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>float ragdoll_force_multiplier = 0.025; //  velocity * ragdoll_force_multiplier is applied to any ragdolls that are created by entities killed by this<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="hit_particle_force_multiplier" type="xsd:decimal" default="0.1">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>float hit_particle_force_multiplier = 0.1; //  hit particle velocity = projectile_velocity * hit_particle_force_multiplier * some randomness<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="blood_count_multiplier" type="xsd:decimal" default="1">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>float blood_count_multiplier = 1; //  how much blood does this projectile cause<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="damage_game_effect_entities" type="xsd:string" default="">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>std::string damage_game_effect_entities; //  a list of game_effects entities separated with ','. e.g. 'data/entities/misc/effect_electrocution.xml,data/entities/misc/effect_on_fire.xml' <br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="never_hit_player" type="NoitaBool" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>bool never_hit_player = 0; //  If 1, does not hit player no matter what herds this and player belong to<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="collect_materials_to_shooter" type="NoitaBool" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>bool collect_materials_to_shooter = 0; //  if 1, looks up the 'who_shot' entity and its MaterialInventoryComponent on destruction and updates it based on the cells destroyed on our explosion.<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="play_damage_sounds" type="NoitaBool" default="1">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>bool play_damage_sounds = 1;<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="mLastFrameDamaged" type="xsd:int" default="-1024">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>int mLastFrameDamaged = -1024;<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="projectile_type" type="PROJECTILE_TYPE" default="PROJECTILE">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>PROJECTILE_TYPE::Enum projectile_type;<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="shell_casing_offset.x" type="xsd:decimal" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>vec2 shell_casing_offset; //  where the shell casing will be created relative to projectile, y is flipped if projectile direction is to the left.<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="shell_casing_offset.y" type="xsd:decimal" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>vec2 shell_casing_offset; //  where the shell casing will be created relative to projectile, y is flipped if projectile direction is to the left.<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="ragdoll_fx_on_collision" type="RAGDOLL_FX" default="NORMAL">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>RAGDOLL_FX::Enum ragdoll_fx_on_collision; //  if not NORMAL, do a special ragdoll<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<!-- Some Unknown Type: EntityID for mWhoShot -->
		<!-- Some Unknown Type: EntityTypeID for mWhoShotEntityTypeID -->
		<xsd:attribute name="mShooterHerdId" type="xsd:int" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>int mShooterHerdId = 0; //  the herdid of mWhoShot, unless friendly fire<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="mStartingLifetime" type="xsd:int" default="0">
			<xsd:annotation>
				<xsd:documentation><![CDATA[```cpp<br>int mStartingLifetime = 0;<br>```]]></xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
