		<!-- Some Unknown Type: ProjectileTriggers for mTriggers -->
		<!-- Some Unknown Type: VEC_ENTITY for mDamagedEntities -->
		<xsd:attribute name="mInitialSpeed" type="xsd:decimal" default="-1">
