<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<!--Builtin-->
	<xsd:simpleType name="Hex8">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9A-Fa-f]{8}|0" />
		</xsd:restriction>
	</xsd:simpleType>
	<!--Builtin-->
	<xsd:complexType name="BiomeAllBiome" mixed="true">
		<xsd:attribute name="color" type="Hex8" use="required"></xsd:attribute>
		<xsd:attribute name="biome_filename" type="xsd:string" use="required"></xsd:attribute>
	</xsd:complexType>
	<xsd:element name="BiomesToLoad">
		<xsd:complexType mixed="true">
			<xsd:sequence minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="Biome" type="BiomeAllBiome"></xsd:element>
			</xsd:sequence>
			<xsd:attribute name="biome_image_map" type="xsd:string"></xsd:attribute>
			<xsd:attribute name="biome_offset_y" type="xsd:int"></xsd:attribute>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
